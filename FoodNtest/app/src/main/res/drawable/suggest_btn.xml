<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >>
        <shape android:shape="oval"  >
            <corners android:radius="360dip" />
            <solid android:color="#ffffff"/>
            <padding
                android:left="7dp"
                android:right="7dp"
                android:top="7dp"
                android:bottom="7dp"/>
        </shape>
    </item>
    <item android:state_focused="true" >>
        <shape android:shape="oval"  >
            <corners android:radius="360dip" />
            <solid android:color="#ffffff"/>
            <padding
                android:left="7dp"
                android:right="7dp"
                android:top="7dp"
                android:bottom="7dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="oval"  >
            <corners android:radius="360dip" />
            <solid android:color="#ffffff"/>
            <padding
                android:left="7dp"
                android:right="7dp"
                android:top="7dp"
                android:bottom="7dp"/>
        </shape>
    </item>
</selector>